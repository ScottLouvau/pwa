    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'lib_wordle'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=lib-wordle"
                ],
                "filter": {
                    "name": "lib_wordle",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'wordle'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wordle",
                    "--package=wordle"
                ],
                "filter": {
                    "name": "wordle",
                    "kind": "bin"
                }
            },
            "_args": ["simulate", "2", "../data/v13.txt", "--total"],
            "args": ["assess", "../data/v13.txt", "clint", "soare", "dough", "dodgy"],
            "cwd": "${workspaceFolder}/bin-wordle"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'wordle'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=wordle",
                    "--package=wordle"
                ],
                "filter": {
                    "name": "wordle",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'wordle-http'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wordle-http",
                    "--package=wordle-http"
                ],
                "filter": {
                    "name": "wordle-http",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'wordle-http'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=wordle-http",
                    "--package=wordle-http"
                ],
